<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">



<menu id="root-menu" label="Openbox 3">
  <separator label="Applications" />
  <item label="Termite">
    <action name="Execute">
      <command>termite</command>
    </action>
  </item>
  <item label="Firefox">
    <action name="Execute">
      <command>firefox</command>
    </action>
  </item>
  <item label="Pulseaudio controller(pavucontrol)">
    <action name="Execute">
      <command>pavucontrol</command>
    </action>
  </item>
  <separator label="System" />
  <menu id="system-menu"/>
  <separator />
  <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
